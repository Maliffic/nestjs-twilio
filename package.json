{
  "name": "@maliffic/nestjs-twilio",
  "version": "1.0.7",
  "description": "Injectable Twilio client for Nestjs",
  "license": "MIT",
  "author": {
    "email": "crucifixx25@gmail.com",
    "name": "Maliffic"
  },
  "repository": "https://github.com/Maliffic/nestjs-twilio",
  "bugs": "https://github.com/Maliffic/nestjs-twilio/issues",
  "private": false,
  "keywords": [
    "nestjs",
    "twilio",
    "sms",
    "client",
    "sdk",
    "typescript",
    "nestjs-twilio",
    "twilio-nestjs"
  ],
  "source": "lib/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:dist": "yarn clean:dist && tsc -p tsconfig.build.json",
    "test:run": "NODE_ENV=test jest",
    "test:watch": "yarn test:run --watch",
    "test:coverage": "yarn clean:test && yarn test:run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix . --ext .js,.ts,.tsx",
    "clean:dist": "rimraf dist",
    "clean:test": "rimraf coverage",
    "prepare": "yarn build:dist"
  },
  "devDependencies": {
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "@nestjs/testing": "7.6.17",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.2",
    "@types/twilio": "^3.19.3",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.7",
    "jest": "26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "6.6.7",
    "ts-jest": "26.5.4",
    "twilio": "3.63.0",
    "typescript": "4.3.2"
  },
  "peerDependencies": {
    "@nestjs/common": "7.6.17",
    "@nestjs/core": "7.6.17",
    "twilio": "3.63.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
